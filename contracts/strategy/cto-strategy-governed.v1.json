{
  "contract_id": "cto-strategy-governed",
  "version": "1.0.0",
  "description": "Deterministic governance contract for CTO Strategy Outputs",
  "execution_mode": "deterministic",
  "input_trust_level": "untrusted",
  "required_sections": [
    "Current State Baseline",
    "Target State (12â€“24 Months)",
    "Assumptions",
    "Constraints",
    "Key Strategic Decisions",
    "Dependencies",
    "Risks and Mitigations",
    "Delivery Roadmap",
    "Ownership and Accountability",
    "Board Summary"
  ],
  "validation_rules": [
    {
      "id": "baseline_present",
      "section": "Current State Baseline",
      "rule": "min_length",
      "value": 200
    },
    {
      "id": "constraints_present",
      "section": "Constraints",
      "rule": "must_include_any",
      "value": ["budget", "security", "skills", "regulatory"]
    },
    {
      "id": "decisions_have_owner",
      "section": "Key Strategic Decisions",
      "rule": "must_include",
      "value": "Owner:"
    },
    {
      "id": "risks_have_mitigations",
      "section": "Risks and Mitigations",
      "rule": "paired_entries",
      "value": true
    },
    {
      "id": "board_summary_present",
      "section": "Board Summary",
      "rule": "min_length",
      "value": 100
    }
  ],
  "risk_classification": {
    "LOW": "All rules satisfied",
    "MODERATE": "One or more non-critical rule failures",
    "HIGH": "Missing baseline, decisions, risks, or ownership"
  },
  "ledger": {
    "record_execution": true,
    "immutable": true
  }
}
