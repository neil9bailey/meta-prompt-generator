{
  "catalogue_version": "v1",
  "principles": [
    "catalogue_entries_are_data_not_logic",
    "no_overlay_or_vendor_is_applied_implicitly",
    "all_references_must_be_explicit_in_the_contract",
    "missing_references_raise_risk_not_runtime_errors",
    "no_scoring_or_recommendation_is_permitted"
  ],
  "enforcement": {
    "validate_on_load": true,
    "block_unknown_fields": true,
    "disallow_opinion_language": true
  },
  "assurance_rules": {
    "multiple_vendors_require_comparative_coverage": true,
    "regulatory_overlays_require_explicit_acknowledgement": true,
    "security_overlays_require_risk_section_presence": true
  }
}
